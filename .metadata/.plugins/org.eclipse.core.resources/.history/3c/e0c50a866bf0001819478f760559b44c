package LinkedList;

public class SumLists {
	static Node head;
	static class Node{
		int data;
		Node next;
		Node(int d){
			data = d;
			next = null;
		}
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		SumLists list = new SumLists();
		list.head = new Node(7);
		list.head.next = new Node(1);
		list.head.next.next = new Node(6);
		list.head.next.next.next = new Node(5);
		list.head.next.next.next.next = new Node(9);
		list.head.next.next.next.next.next = new Node(2);
		
	}

}
